varsList.add(&quot;depotOrderHeader_isIncludeNotes&quot;);
varsList.add(&quot;depotOrderHeader_error&quot;); //precheck error
varsList.add(&quot;depotOrderHeader_cq_error&quot;); //create quote error

varsList.add(&quot;domainCode&quot;);

// Set default values
varsList.each {
	if (vars.get(&quot;${it}&quot;) == null) vars.put(&quot;${it}&quot;, defaultValue);
}</stringProp>
