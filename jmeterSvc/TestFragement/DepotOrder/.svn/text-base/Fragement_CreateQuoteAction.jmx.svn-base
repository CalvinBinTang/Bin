            <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="Update JSON using JavaScript" enabled="true">
              <stringProp name="scriptLanguage">javascript</stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="cacheKey">false</stringProp>
              <stringProp name="script">var requestData = vars.get(&quot;RequestData&quot;);
var prefix = &quot;depotOrderHeader_&quot;;

//Parse the JSON to get a JavaScript data object
var objRequest = JSON.parse(requestData);

//Update the value of the data object
if (vars.get(&quot;depotOrderHeader_isPrintQuote&quot;) != &quot;[N/A]&quot;) objRequest.depotQuoteCreates[0].isPrintQuote = vars.get(&quot;depotOrderHeader_isPrintQuote&quot;);
if (vars.get(&quot;depotOrderHeader_quoteDate&quot;) != &quot;[N/A]&quot;) objRequest.depotQuoteCreates[0].quoteDate = vars.get(&quot;depotOrderHeader_quoteDate&quot;);
if (vars.get(&quot;depotOrderHeader_expireDate&quot;) != &quot;[N/A]&quot;) objRequest.depotQuoteCreates[0].expireDate = vars.get(&quot;depotOrderHeader_expireDate&quot;);
if (vars.get(&quot;depotOrderHeader_isLoadPartsList&quot;) != &quot;[N/A]&quot;) objRequest.depotQuoteCreates[0].isLoadPartsList = vars.get(&quot;depotOrderHeader_isLoadPartsList&quot;);
if (vars.get(&quot;depotOrderHeader_isExplodeRouting&quot;) != &quot;[N/A]&quot;) objRequest.depotQuoteCreates[0].isExplodeRouting = vars.get(&quot;depotOrderHeader_isExplodeRouting&quot;);
if (vars.get(&quot;depotOrderHeader_isIncludeExistingSvcActivities&quot;) != &quot;[N/A]&quot;) objRequest.depotQuoteCreates[0].isIncludeExistingSvcActivities = vars.get(&quot;depotOrderHeader_isIncludeExistingSvcActivities&quot;);
if (vars.get(&quot;depotOrderHeader_isIncludeNotes&quot;) != &quot;[N/A]&quot;) objRequest.depotQuoteCreates[0].quoteDate = vars.get(&quot;depotOrderHeader_isIncludeNotes&quot;);

//Convert the data object back to a JSON string 
var strUpdatedRequest = JSON.stringify(objRequest);

//Update ${RequestData} to have the value of the updated JSON
vars.put(&quot;RequestData&quot;, strUpdatedRequest);</stringProp>
            </JSR223PreProcessor>
