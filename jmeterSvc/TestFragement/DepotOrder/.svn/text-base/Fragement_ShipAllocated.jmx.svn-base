            <stringProp name="parameters"></stringProp>
            <stringProp name="script">var responseData = vars.get(&quot;SubmitResult&quot;);
var objRequest = JSON.parse(responseData);
var failureMessage = &quot;&quot;;

// All the fields in dataset should be set here
var re = /${shipAllocated_validate_precheckErrMsg}/;
if (&quot;${shipAllocated_validate_precheckErrMsg}&quot; != &quot;[N/A]&quot;){	   
	if (re.exec(responseData) == null) {
		failureMessage = failureMessage + &quot;Expect error ${shipAllocated_validate_precheckErrMsg} but not found in response data. /n&quot;;
	}
}
//&quot;hasLinesToShip&quot;:true

AssertionResult.setFailureMessage(failureMessage);
AssertionResult.setFailure(failureMessage != &quot;&quot;);
</stringProp>
