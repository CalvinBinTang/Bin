            <stringProp name="cacheKey">false</stringProp>
            <stringProp name="script">var requestData = vars.get(&quot;RequestData&quot;);
var objRequest = JSON.parse(requestData);

// All the fields in dataset should be set here
if (&quot;${customerQuoteHeaderComments_combinedComment}&quot; != &quot;[N/A]&quot;) objRequest.customerQuoteHeaderComments[0].combinedComment= &quot;${customerQuoteHeaderComments_combinedComment}&quot;;
if (&quot;${customerQuoteHeaderComments_isPrintOnQuote}&quot; != &quot;[N/A]&quot;) objRequest.customerQuoteHeaderComments[0].isPrintOnQuote= &quot;${customerQuoteHeaderComments_isPrintOnQuote}&quot;;
if (&quot;${customerQuoteHeaderComments_pageNumber}&quot; != &quot;[N/A]&quot;) objRequest.customerQuoteHeaderComments[0].pageNumber= &quot;${customerQuoteHeaderComments_pageNumber}&quot;;
objRequest.customerQuoteHeaderComments[0].storedPageNumber= &quot;${Output_customerQuoteHeaderComments_pageNumber}&quot;;

var strUpdatedRequest = JSON.stringify(objRequest);
vars.put(&quot;RequestData&quot;, strUpdatedRequest);</stringProp>
