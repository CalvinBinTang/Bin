          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">var requestData = vars.get(&quot;RequestData&quot;);
var objRequest = JSON.parse(requestData);
var failureMessage = &quot;&quot;;
var defaultValue = &quot;[N/A]&quot;;
var LaborCount = vars.get(&quot;LaborCount&quot;);
// All the fields in dataset should be set here
// Below code block is only for depotOrderHeaders[0] validation, please use other prefix and code to cover other scenarios.
var LaborTotalExtendedPrice = 0.00;
for(var i = 0; i &lt; LaborCount ; i++){
	LaborTotalExtendedPrice = LaborTotalExtendedPrice + eval(&quot;objRequest.custQuoteLineLaborUIs[&quot;+i+&quot;].extendedPrice&quot;);

}

LaboTotalExtendedPrice = parseFloat(LaborTotalExtendedPrice).toFixed(2);

vars.put(&quot;LaborTotalExtendedPrice&quot;,LaborTotalExtendedPrice);
</stringProp>
