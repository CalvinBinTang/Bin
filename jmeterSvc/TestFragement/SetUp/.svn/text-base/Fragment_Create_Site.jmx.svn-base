            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">false</stringProp>
            <stringProp name="script">var strRequest = vars.get(&quot;RequestData&quot;);
//Parse the Response JSON to get a JavaScript data object
var objResponse = JSON.parse(strRequest);

objResponse.sites[0].siteCode=&quot;${site_siteCode}&quot;;
objResponse.sites[0].defaultInventoryStatus=&quot;${site_InvStatus}&quot;;
if (&quot;${site_Description}&quot; != &quot;[N/A]&quot;) objResponse.sites[0].description=&quot;${site_Description}&quot;;

var strUpdatedResponse =JSON.stringify(objResponse);
vars.put(&quot;RequestData&quot;,strUpdatedResponse);
</stringProp>
          </JSR223PreProcessor>
