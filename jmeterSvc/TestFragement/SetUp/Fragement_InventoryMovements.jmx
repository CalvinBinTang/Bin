          <stringProp name="cacheKey">false</stringProp>
          <stringProp name="script">String defaultValue = &quot;[N/A]&quot;;
List varsList = [];

// Add module variables here
//varsList.add(&quot;[prefix]_[var_name]&quot;);
varsList.add(&quot;domainCode&quot;);
varsList.add(&quot;InventoryMovements_inventoryMovementCode&quot;);
varsList.add(&quot;InventoryMovements_description&quot;);
varsList.add(&quot;InventoryMovements_transactionFlowCode&quot;);//0 Internal
varsList.add(&quot;InventoryMovements_transactionFlowDescription&quot;);
varsList.add(&quot;InventoryMovements_transactionFlowID&quot;);//0 Internal
varsList.add(&quot;InventoryMovements_transactionType&quot;);


// Set default values
varsList.each {
	if (vars.get(&quot;${it}&quot;) == null) vars.put(&quot;${it}&quot;, defaultValue);
}</stringProp>
