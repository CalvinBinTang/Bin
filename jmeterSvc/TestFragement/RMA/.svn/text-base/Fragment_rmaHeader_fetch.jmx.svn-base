          <stringProp name="cacheKey">false</stringProp>
          <stringProp name="script">String defaultValue = &quot;[N/A]&quot;;
List varsList = [];

// URL Paremeters
varsList.add(&quot;rmaHeader_rmaNumber&quot;);
varsList.add(&quot;rmaHeader_url_parameters&quot;);


// Inputs
varsList.add(&quot;rmaHeader_update_s_purchaseOrderNumber&quot;); //purchase order number

varsList.add(&quot;rmaHeader_update_i_isForecastConsumed&quot;); //contract Visit


// Fields for validation
varsList.add(&quot;rmaHeader_validate_shipVia&quot;); //sold-To


varsList.each {
if (vars.get(&quot;${it}&quot;) == null) vars.put(&quot;${it}&quot;, defaultValue);
}</stringProp>
