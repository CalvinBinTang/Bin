            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">var requestData = vars.get(&quot;RequestData&quot;);
var objRequest = JSON.parse(requestData);
var defaultValue = &quot;[N/A]&quot;;
var failureMessage = &quot;&quot;;

var temp1=&quot;${rmaHeader_url_fieldName}&quot;;
var temp2=&quot;${rmaHeader_fieldValue}&quot;;
eval( &quot;objRequest.rmaHeaders[0].&quot; + temp1 + &quot;=&apos;&quot; +temp2+&quot;&apos;&quot;);

/*if(&quot;${contractLine_confirmation}&quot;==&quot;true&quot;)
