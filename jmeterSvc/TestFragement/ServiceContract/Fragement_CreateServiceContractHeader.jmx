        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="set default values" enabled="true">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">false</stringProp>
          <stringProp name="script">String defaultValue = &quot;[N/A]&quot;;
List varsList = [];

// Add module variables here
//varsList.add(&quot;[prefix]_[var_name]&quot;);
varsList.add(&quot;serviceContract_customerCode&quot;); //sold-to
varsList.add(&quot;serviceContract_contract&quot;);
varsList.add(&quot;serviceContract_startDate&quot;);
varsList.add(&quot;serviceContract_endDate&quot;);
varsList.add(&quot;serviceContract_serviceType&quot;);
varsList.add(&quot;serviceContract_purchaseOrder&quot;);
varsList.add(&quot;serviceContract_orderDate&quot;);
varsList.add(&quot;serviceContract_cancelDate&quot;);
varsList.add(&quot;serviceContract_currencyCode&quot;); //currency
varsList.add(&quot;serviceContract_siteCode&quot;); //site
varsList.add(&quot;serviceContract_siteEntityCode&quot;); //entity
varsList.add(&quot;serviceContract_masterContract&quot;);
varsList.add(&quot;serviceContract_type&quot;);
varsList.add(&quot;serviceContract_visits&quot;); //contract visit
varsList.add(&quot;serviceContract_priceList&quot;);
varsList.add(&quot;serviceContract_isBillEndUsers&quot;);
varsList.add(&quot;serviceContract_isBlanketContract&quot;);
varsList.add(&quot;serviceContract_creditTerms&quot;);
varsList.add(&quot;serviceContract_isPeriodBasedBilling&quot;);
varsList.add(&quot;serviceContract_isProratePartialPeriods&quot;);
varsList.add(&quot;serviceContract_revenueType&quot;);
varsList.add(&quot;serviceContract_isBillArrears&quot;);

// Set default values
varsList.each {
	if (vars.get(&quot;${it}&quot;) == null) vars.put(&quot;${it}&quot;, defaultValue);
}</stringProp>
        </JSR223Sampler>
