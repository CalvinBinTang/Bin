          <JSR223Assertion guiclass="TestBeanGUI" testclass="JSR223Assertion" testname="Data Assertion" enabled="true">
            <stringProp name="cacheKey">false</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">var requestData = vars.get(&quot;RequestData&quot;);
var objRequest = JSON.parse(requestData);
var failureMessage = &quot;&quot;;
var defaultValue = &quot;[N/A]&quot;;

var temp = prev.getResponseDataAsString();
	//vars.put(&quot;temp&quot;, temp);
var errormessage = JSON.parse(temp);
	
if(&quot;${serviceContract_error}&quot;!=&quot;[N/A]&quot;)
//if(&quot;${serviceContract_error}&quot;!=&quot;[N/A]&quot; &amp;&amp; &quot;contractLine_confirmation&quot; != &quot;true&quot;)
{
	var findMessage = false ;
	for(var i in errormessage.submitResult.errors)
	{
  		var actValue = eval(&quot;errormessage.submitResult.errors[&quot;+i+&quot;].message&quot;);
  		//failureMessage = failureMessage +actValue+ &quot;\n&quot;;
  		if(actValue == &quot;${serviceContract_error}&quot;)
  		 {
  		 	findMessage = true ;
  		 	//failureMessage = failureMessage + &quot;find=true\n&quot;;
  		 	break;
  		 }
	}
  	if(findMessage == false)
  		failureMessage = failureMessage + &quot;Expect &quot; + &quot;${serviceContract_error}&quot; + &quot; but can not find\n&quot;;
  	if(errormessage.submitResult.success == &quot;true&quot;)
  		failureMessage = failureMessage + &quot;Expect &quot; + &quot;${serviceContract_error}&quot; + &quot; but delete successfully\n&quot;;
}
else if(errormessage.submitResult.success == &quot;false&quot;)
failureMessage = failureMessage + &quot;Expect no error but have errors\n&quot;;


AssertionResult.setFailureMessage(failureMessage);
AssertionResult.setFailure(failureMessage != &quot;&quot;);
</stringProp>
            <stringProp name="scriptLanguage">javascript</stringProp>
