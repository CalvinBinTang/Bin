          <JSR223Assertion guiclass="TestBeanGUI" testclass="JSR223Assertion" testname="Data Assertion" enabled="true">
            <stringProp name="cacheKey">false</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">var requestData = vars.get(&quot;RequestData&quot;);
var objRequest = JSON.parse(requestData);
var failureMessage = &quot;&quot;;
var defaultValue = &quot;[N/A]&quot;;
var prefix2 = &quot;contractLine_validate_&quot;;


// All the fields in dataset should be set here
// Below code block is only for serviceContractHeaders[0] validation, please use other prefix and code to cover other scenarios.
var iter = vars.getIterator();

while (iter.hasNext()){
	//var varName = o.name;
	var e = iter.next();
	var varName = e.getKey();
	// if the prefix matches
	if (varName.startsWith(prefix2) &amp;&amp; e.getValue() != defaultValue){
		var actValue = eval(&quot;objRequest.serviceContractLines[0].&quot; + varName.replace(prefix2,&quot;&quot;));
		if(actValue != e.getValue()){
			failureMessage = failureMessage + &quot;Expect &quot; + varName.replace(prefix2,&quot;&quot;) +&quot; = &quot; + e.getValue() + &quot; but got &quot; + actValue + &quot;\n&quot;;
		}
	}
}

if(&quot;${contractLine_message}&quot;!=&quot;[N/A]&quot;)
{
	var temp = prev.getResponseDataAsString();
	var errormessage = JSON.parse(temp);
	var findMessage = false ;
	for(var i in errormessage.submitResult.errors)
	{
  		var actValue = eval(&quot;errormessage.submitResult.errors[&quot;+i+&quot;].message&quot;);
  		//failureMessage = failureMessage +actValue+ &quot;\n&quot;;
  		if( actValue == &quot;${contractLine_message}&quot; )
  		{
  		 	findMessage = true ;
  		 	//failureMessage = failureMessage + &quot;find=true\n&quot;;
  			break;
  	 	}
	}
 	if(findMessage == false)
 	failureMessage = failureMessage + &quot;Expect ${contractLine_message}, but can not find\n&quot;;
}
else //no message=success
{
	var temp = prev.getResponseDataAsString();
	var errormessage = JSON.parse(temp);
	if(errormessage.submitResult.success != true)
	failureMessage = failureMessage + &quot;Expect success = true , but get &quot;+errormessage.submitResult.success+&quot;\n&quot;;
}

AssertionResult.setFailureMessage(failureMessage);
AssertionResult.setFailure(failureMessage != &quot;&quot;);</stringProp>
            <stringProp name="scriptLanguage">javascript</stringProp>
