          <JSR223Assertion guiclass="TestBeanGUI" testclass="JSR223Assertion" testname="JSR223 Assertion" enabled="true">
            <stringProp name="scriptLanguage">javascript</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">false</stringProp>
            <stringProp name="script">var failureMessage = &quot;&quot;;
var temp = prev.getResponseDataAsString();
var errormessage = JSON.parse(temp);

if (&quot;${ContractLine_error}&quot; !== &quot;[N/A]&quot;) 
{ 
	var findMessage = false ;
	for(var i in errormessage.submitResult.errors)
	{
  		var actValue = eval(&quot;errormessage.submitResult.errors[&quot;+i+&quot;].message&quot;);
  		if( actValue == &quot;${ContractLine_error}&quot; )
  		{
  		 	findMessage = true ;ContractLine_error
  			break;
  	 	}
	}
 	if(findMessage == false)
 	failureMessage = failureMessage + &quot;Expect ${ContractLine_error}, but can not find\n&quot;;
}
else if(errormessage.submitResult.success != true)
{
	failureMessage = failureMessage + &quot;Expect true but got &quot;+errormessage.submitResult.success+&quot;\n&quot;;
}


AssertionResult.setFailureMessage(failureMessage);
AssertionResult.setFailure(failureMessage != &quot;&quot;);
</stringProp>
