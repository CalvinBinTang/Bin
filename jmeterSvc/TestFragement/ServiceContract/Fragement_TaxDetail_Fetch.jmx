          <stringProp name="script">String defaultValue = &quot;[N/A]&quot;;
List varsList = [];

// URL Paremeters

varsList.add(&quot;taxDetail_contractNumber&quot;);
varsList.add(&quot;taxDetail_contractLine&quot;);
varsList.add(&quot;taxDetail_associatedDoc&quot;);
varsList.add(&quot;taxDetail_trailerCode&quot;);
varsList.add(&quot;taxDetail_taxCode&quot;);
varsList.add(&quot;taxDetail_validate_contractTaxLineText&quot;);
varsList.add(&quot;taxDetail_validate_taxCode&quot;);
varsList.add(&quot;taxDetail_validate_taxRate&quot;);
varsList.add(&quot;taxDetail_validate_taxEnvironment&quot;);
varsList.add(&quot;taxDetail_validate_taxableBaseAmount&quot;);
varsList.add(&quot;taxDetail_validate_nonTaxableBaseAmount&quot;);
varsList.add(&quot;taxDetail_validate_taxAmount&quot;);
varsList.add(&quot;taxDetail_validate_recoverableTaxAmount&quot;);
varsList.add(&quot;taxDetail_validate_nonRecoverableTaxAmount&quot;);

varsList.each 
{
	if (vars.get(&quot;${it}&quot;) == null)
	{
		if(&quot;${it}&quot;.startsWith(&quot;taxDetail_&quot;)) vars.put(&quot;${it}&quot;, &quot;&quot;);
		if(&quot;${it}&quot;.startsWith(&quot;taxDetail_validate_&quot;)) vars.put(&quot;${it}&quot;, defaultValue);
	}
}

if(vars.get(&quot;taxDetail_validate_contractTaxLineText&quot;) == &quot;Header&quot;) //Tax by line = false
{
	vars.put(&quot;taxDetail_contractLine&quot;,&quot;0&quot;);
}
else if(vars.get(&quot;taxDetail_trailerCode&quot;) != &quot;&quot;)
{
	vars.put(&quot;taxDetail_contractLine&quot;,&quot;99999999&quot;); //trailer code&apos;s tax line is 99999999
}

</stringProp>
