          <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="save" enabled="true">
            <stringProp name="scriptLanguage">javascript</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">false</stringProp>
            <stringProp name="script">//vars.put(&quot;contractLine_line&quot;, &quot;${contractLine_contractLine}&quot;);

var requestData = vars.get(&quot;RequestData&quot;);
var objRequest = JSON.parse(requestData);
var defaultValue = &quot;[N/A]&quot;;
var failureMessage = &quot;&quot;;
var prefixs = &quot;contractLine_s_&quot;;
var prefixi = &quot;contractLine_i_&quot;;

objRequest.serviceContractLines[0].SCLineLotSerialDetails.unshift({});

var iter = vars.getIterator();

while (iter.hasNext())
{

	var e = iter.next();
	var varName = e.getKey();

	if (e.getValue() != defaultValue)
	{		
		if(varName.startsWith(prefixs))
		{
			//log.info(varName+&quot;\n&quot;);
			//log.info(e.getValue(+&quot;\n&quot;));
			var temp = varName.replace(prefixs,&quot;&quot;);
			eval(&quot;objRequest.serviceContractLines[0].SCLineLotSerialDetails[0].&quot; + temp + &quot;= &apos;&quot;+e.getValue()+&quot;&apos;&quot;);
		}
		else if(varName.startsWith(prefixi))
		{
			var temp = varName.replace(prefixi,&quot;&quot;);
			eval(&quot;objRequest.serviceContractLines[0].SCLineLotSerialDetails[0].&quot; + temp + &quot;=&quot;+e.getValue());
		}
	}
}

var strUpdatedRequest = JSON.stringify(objRequest);
vars.put(&quot;RequestData&quot;, strUpdatedRequest);</stringProp>
