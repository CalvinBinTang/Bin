// Add module variables here
//varsList.add(&quot;[prefix]_[var_name]&quot;);
varsList.add(&quot;serviceOrderLine_domainCode&quot;);
varsList.add(&quot;serviceOrderLine_serviceOrderNumber&quot;);
varsList.add(&quot;serviceOrderLine_itemCode&quot;);
varsList.add(&quot;serviceOrderLine_serial&quot;);
varsList.add(&quot;serviceOrderLine_workCode&quot;);
varsList.add(&quot;serviceOrderLine_reference&quot;);
varsList.add(&quot;serviceOrderLine_BillOfMaterialCode&quot;);
varsList.add(&quot;serviceOrderLine_serviceOrderLine&quot;);
varsList.add(&quot;serviceOrderLine_RoutingCode&quot;);
varsList.add(&quot;serviceOrderLine_statusCode&quot;);
varsList.add(&quot;serviceOrderLine_Assigned&quot;);
varsList.add(&quot;serviceOrderLine_fixedPrice&quot;);
varsList.add(&quot;serviceOrderLine_quantity&quot;);
varsList.add(&quot;serviceOrderLine_siteCode&quot;);


// Set default values
varsList.each {
	if (vars.get(&quot;${it}&quot;) == null) vars.put(&quot;${it}&quot;, defaultValue);
}</stringProp>
