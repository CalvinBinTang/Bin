          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">String defaultValue = &quot;[N/A]&quot;;
List varsList = [];

// Add module variables here
//varsList.add(&quot;[prefix]_[var_name]&quot;);
varsList.add(&quot;actionRequestHeader_itemCode&quot;);
varsList.add(&quot;actionRequestHeader_contactType&quot;);
varsList.add(&quot;actionRequestHeader_contactCode&quot;); 
varsList.add(&quot;actionRequestHeader_severity&quot;); 
varsList.add(&quot;actionRequestHeader_requestDate&quot;); 

varsList.add(&quot;domainCode&quot;);

// Set default values
varsList.each {
	if (vars.get(&quot;${it}&quot;) == null) vars.put(&quot;${it}&quot;, defaultValue);
}</stringProp>
