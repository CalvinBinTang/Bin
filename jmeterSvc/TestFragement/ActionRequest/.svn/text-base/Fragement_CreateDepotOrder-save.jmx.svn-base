          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">String defaultValue = &quot;[N/A]&quot;;
List varsList = [];

varsList.add(&quot;DepotOrderNumber&quot;);
//varsList.add(&quot;actiondata&quot;);
varsList.add(&quot;domainCode&quot;);
varsList.add(&quot;errorMsg&quot;);
varsList.add(&quot;depotOrderCreate_purchaseOrderNumber&quot;);
varsList.add(&quot;depotOrderCreate_replacementLotSerial&quot;);

// Set default values
varsList.each {
	if (vars.get(&quot;${it}&quot;) == null) vars.put(&quot;${it}&quot;, defaultValue);
}</stringProp>
        </JSR223Sampler>
