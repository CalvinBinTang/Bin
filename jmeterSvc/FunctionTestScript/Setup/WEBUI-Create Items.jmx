              <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="Update JSON using JavaScript" enabled="true">
                <stringProp name="scriptLanguage">javascript</stringProp>
                <stringProp name="parameters"></stringProp>
                <stringProp name="filename"></stringProp>
                <stringProp name="cacheKey">false</stringProp>
                <stringProp name="script">var strResponse = vars.get(&quot;RequestData&quot;);

var objResponse = JSON.parse(strResponse);

objResponse.items[0].ABCClass =&quot;${Class}&quot;;
objResponse.items[0].itemCode =&quot;${Item}&quot;;
objResponse.items[0].description =&quot;${Description}&quot;;
objResponse.items[0].unitOfMeasure =&quot;${unitOfMeasure}&quot;;
objResponse.items[0].itemStatus =&quot;${Status}&quot;;
objResponse.items[0].siteCode =&quot;${Site}&quot;;
objResponse.items[0].productLine =&quot;${ProdLine}&quot;;
if (&quot;${Price}&quot; != &quot;&quot;) {objResponse.items[0].listPrice =${Price};}
else {objResponse.items[0].listPrice =0;}
if (&quot;${ShipWeight}&quot; != &quot;&quot;) {objResponse.items[0].shipWeight =${ShipWeight};}
else {objResponse.items[0].shipWeight =0;}
if (&quot;${isTaxable}&quot; != &quot;&quot;){objResponse.items[0].isTaxable =!!${isTaxable};}




if (&quot;${GLBurden}&quot; != &quot;&quot;) {objResponse.items[0].GLCostTotal.GLCostDetails[0].thisLevelCost =${GLBurden};}
else {objResponse.items[0].GLCostTotal.GLCostDetails[0].thisLevelCost =0;}
if (&quot;${GLLabor}&quot; != &quot;&quot;) {objResponse.items[0].GLCostTotal.GLCostDetails[1].thisLevelCost =${GLLabor};}
else {objResponse.items[0].GLCostTotal.GLCostDetails[1].thisLevelCost =0;}
if (&quot;${GLMaterial}&quot; != &quot;&quot;) {objResponse.items[0].GLCostTotal.GLCostDetails[2].thisLevelCost =${GLMaterial};}
else {objResponse.items[0].GLCostTotal.GLCostDetails[2].thisLevelCost =0;}
if (&quot;${GLOverhead}&quot; != &quot;&quot;) {objResponse.items[0].GLCostTotal.GLCostDetails[3].thisLevelCost =${GLOverhead};}
else {objResponse.items[0].GLCostTotal.GLCostDetails[3].thisLevelCost =0;}
if (&quot;${GLSubcontract}&quot; != &quot;&quot;) {objResponse.items[0].GLCostTotal.GLCostDetails[4].thisLevelCost =${GLSubcontract};}
else {objResponse.items[0].GLCostTotal.GLCostDetails[4].thisLevelCost =0;}


var strUpdatedResponse = JSON.stringify(objResponse);


vars.put(&quot;RequestData&quot;, strUpdatedResponse);</stringProp>
              </JSR223PreProcessor>
