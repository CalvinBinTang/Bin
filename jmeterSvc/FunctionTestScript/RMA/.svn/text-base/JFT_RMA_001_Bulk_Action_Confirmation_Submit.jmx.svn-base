	SimpleDateFormat sdf= new SimpleDateFormat(&quot;yyyy-MM-dd&apos;T00:00:00.000Z&apos;&quot;);
	
	TimeZone tz=TimeZone.getTimeZone(&quot;America/New_York&quot;);
	log.info(tz.getID());
	sdf.setTimeZone(tz);//Sets the time zone for the calendar of this DateFormat object.
	String TimeZoneDate=sdf.format(today);
	
	log.info(&quot;Server Timezone is &quot;+TimeZoneDate);
	vars.put(&quot;serverDate&quot;, TimeZoneDate);
	
	Calendar cal = Calendar.getInstance(); // get Calendar instance
	cal.setTime(today); // set Calendar&apos;s time to be today
	int i = Integer.parseInt(dueDate.substring(5)); 
	cal.add(Calendar.DAY_OF_YEAR,i);
	Date date1 = cal.getTime();
	vars.put(&quot;dueDate&quot;,sdf.format(date1)); 
}</stringProp>
