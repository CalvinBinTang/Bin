            <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="Verify the combined customer quote line default value" enabled="true">
              <stringProp name="cacheKey">false</stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="script">var requestData = vars.get(&quot;RequestData&quot;);
var failureMessage = &quot;&quot;;
//Parse the JSON to get a JavaScript data object
var objRequest = JSON.parse(requestData);


if (objRequest.customerQuoteLines[0].referenceNumber != &quot;&quot;) { failureMessage = failureMessage + &quot;Expect referenceNumber blank but got &quot; + objRequest.customerQuoteLines[0].referenceNumber}
if (objRequest.customerQuoteLines[0].referenceType !=&quot;1&quot;) { failureMessage = failureMessage + &quot;Expect referenceType 1 but got &quot; + objRequest.customerQuoteLines[0].referenceType}
if (objRequest.customerQuoteLines[0].referenceTypeDescription !=&quot;Service&quot;) { failureMessage = failureMessage + &quot;Expect referenceTypeDescription Service but got &quot; + objRequest.customerQuoteLines[0].referenceTypeDescription}
if (objRequest.customerQuoteLines[0].itemRepairTypeDisplayValue !=&quot;&quot;) { failureMessage = failureMessage + &quot;Expect itemRepairTypeDisplayValue blank but got &quot; + objRequest.customerQuoteLines[0].itemRepairTypeDisplayValue}
if (objRequest.customerQuoteLines[0].workCode !=&quot;3001&quot;) { failureMessage = failureMessage + &quot;Expect workCode 3001 but got &quot; + objRequest.customerQuoteLines[0].workCode}
if (objRequest.customerQuoteLines[0].freightMinimumWeight !=&quot;2&quot;) { failureMessage = failureMessage + &quot;Expect freightMinimumWeight 2 but got &quot; + objRequest.customerQuoteLines[0].freightMinimumWeight}
if (objRequest.customerQuoteLines[0].freightListCode !=&quot;10TXFRT&quot;) { failureMessage = failureMessage + &quot;Expect freightListCode 10TXFRT but got &quot; + objRequest.customerQuoteLines[0].freightListCode}
if (objRequest.customerQuoteLines[0].isFixedPrice !=&quot;0&quot;) { failureMessage = failureMessage + &quot;Expect isFixedPrice false but got &quot; + objRequest.customerQuoteLines[0].isFixedPrice}
if (objRequest.customerQuoteLines[0].fixedPrice !=&quot;0&quot;) { failureMessage = failureMessage + &quot;Expect fixedPrice 0 but got &quot; + objRequest.customerQuoteLines[0].fixedPrice}
if (objRequest.customerQuoteLines[0].endUserCode !=&quot;10c1001&quot;) { failureMessage = failureMessage + &quot;Expect endUserCode 10c1001 but got &quot; + objRequest.customerQuoteLines[0].endUserCode}
AssertionResult.setFailureMessage(failureMessage);
AssertionResult.setFailure(failureMessage != &quot;&quot;);
</stringProp>
              <stringProp name="scriptLanguage">javascript</stringProp>
            </JSR223PostProcessor>
